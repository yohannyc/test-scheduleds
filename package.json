{
  "name": "bilo-server",
  "version": "1.0.0",
  "description": "Backend project for bilo-app",
  "main": "src/index.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "start": "npm run compile && node ./dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "bilo",
    "server"
  ],
  "author": "bilo",
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "express": "^4.18.2",
    "firebase-admin": "^11.5.0",
    "mongoose": "^7.0.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.5",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "nodemon": "^2.0.21",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "18.16.0"
  }
}
